      <frameworkAssembly assemblyName="System" targetFramework="netmf42, netmf43, netmf44" />
      <frameworkAssembly assemblyName="System.Net.Security" targetFramework="netmf42, netmf43, netmf44" />
    </frameworkAssemblies>
