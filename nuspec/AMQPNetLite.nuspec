    <title>AMQP.Net Lite</title>
    <version>0.0.0</version>
    <authors>xinchen</authors>
    <owners>Microsoft</owners>
    <licenseUrl>https://github.com/Azure/amqpnetlite/blob/master/license.txt</licenseUrl>
