      </group>
      <group targetFramework="net45" />
      <group targetFramework="netcore451" />
      <group targetFramework="uap10.0" />
      <group targetFramework="netstandard1.3">
        <dependency id="AMQPNetLite.Core" version="2.1.6" />
      </group>
    </dependencies>
  </metadata>
