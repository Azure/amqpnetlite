<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>AMQPNetMicro</id>
    <title>A compact version of AMQP.Net Lite for NETMF</title>
    <version>0.0.0</version>
    <authors>xinchen</authors>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/Azure/amqpnetlite</projectUrl>
    <repository type="git" url="https://github.com/Azure/amqpnetlite" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This is a compact version of AMQP.Net Lite for .Net Micro Framework on memory/space constrained devices.</description>
    <readme>docs\README.md</readme>
    <copyright>Copyright 2014</copyright>
    <tags>AMQP netmf</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.SPOT.Native" targetFramework="netmf42, netmf43, netmf44" />
      <frameworkAssembly assemblyName="Microsoft.SPOT.Net.Security" targetFramework="netmf42, netmf43, netmf44" />
      <frameworkAssembly assemblyName="System" targetFramework="netmf42, netmf43, netmf44" />
      <frameworkAssembly assemblyName="System.Net.Security" targetFramework="netmf42, netmf43, netmf44" />
    </frameworkAssemblies>
    <dependencies>
      <group targetFramework="netmf44" />
    </dependencies>
  </metadata>
  <files>
    <file src="README.md" target="docs\" />
    <file src="bin\Release\Amqp.Micro.NetMF44\*.*" target="lib\netmf44" />
    <file src="bin\Release\Amqp.Micro.NetMF44\le\*.p*" target="lib\netmf44\le" />
    <file src="bin\Release\Amqp.Micro.NetMF44\be\*.p*" target="lib\netmf44\be" />
  </files>
</package>