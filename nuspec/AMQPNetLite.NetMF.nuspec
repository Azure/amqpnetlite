<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>AMQPNetLite.NetMF</id>
    <title>AMQP.Net Lite for NetMF</title>
    <version>0.0.0</version>
    <authors>xinchen</authors>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/Azure/amqpnetlite</projectUrl>
    <repository type="git" url="https://github.com/Azure/amqpnetlite" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This is a lightweight AMQP 1.0 library for .Net Micro Framework (netmf). A more compact version
      for NETMF is available in the AmqpNetMicro package.
    </description>
    <readme>docs\README.md</readme>
    <copyright>Copyright 2014</copyright>
    <tags>AMQP net netmf</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.SPOT.Native" targetFramework="netmf" />
      <frameworkAssembly assemblyName="Microsoft.SPOT.Net.Security" targetFramework="netmf" />
      <frameworkAssembly assemblyName="System" targetFramework="netmf" />
      <frameworkAssembly assemblyName="System.Net.Security" targetFramework="netmf42" />
    </frameworkAssemblies>
    <dependencies>
      <group targetFramework="netmf44" />
    </dependencies>
  </metadata>
  <files>
    <file src="README.md" target="docs\" />
    <file src="bin\Release\Amqp.NetMF44\Amqp.NetMF.dll" target="lib\netmf44" />
    <file src="bin\Release\Amqp.NetMF44\Amqp.NetMF.pdb" target="lib\netmf44" />
    <file src="bin\Release\Amqp.NetMF44\Amqp.NetMF.xml" target="lib\netmf44" />
    <file src="bin\Release\Amqp.NetMF44\be\Amqp.NetMF.pe" target="lib\netmf44\be" />
    <file src="bin\Release\Amqp.NetMF44\be\Amqp.NetMF.pdbx" target="lib\netmf44\be" />
    <file src="bin\Release\Amqp.NetMF44\le\Amqp.NetMF.pe" target="lib\netmf44\le" />
    <file src="bin\Release\Amqp.NetMF44\le\Amqp.NetMF.pdbx" target="lib\netmf44\le" />
  </files>
</package>