{
  "metadata": [
    {
      "src": [
        {
          "files": [ "csproj/Amqp.Net.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ],
          "src": "../"
        }
      ],
     "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**/*.yml" ]
      },
      {
        "files": [ "articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ]
      }
    ],
    "dest": "../build/_site",
    "template": [ "default", "template" ],
    "globalMetadata": {
      "_disableContribution": true
    }
  }
}